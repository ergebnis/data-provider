{
  "name": "ergebnis/data-provider",
  "description": "Provides generic data providers for use with phpunit/phpunit.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "phpunit",
    "data-provider"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/data-provider",
  "support": {
    "issues": "https://github.com/ergebnis/data-provider/issues",
    "source": "https://github.com/ergebnis/data-provider"
  },
  "require": {
    "php": "~7.4.0 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "fakerphp/faker": "^1.21.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.45.0",
    "ergebnis/license": "^2.6.0",
    "ergebnis/php-cs-fixer-config": "^6.43.0",
    "ergebnis/phpunit-slow-test-detector": "^2.19.0",
    "infection/infection": "~0.26.6",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^1.12.10",
    "phpstan/phpstan-deprecation-rules": "^1.2.1",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpstan/phpstan-strict-rules": "^1.6.1",
    "phpunit/phpunit": "^9.6.19",
    "rector/rector": "^1.2.10"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\DataProvider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\DataProvider\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "7.4.33"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "3.4-dev"
    },
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
