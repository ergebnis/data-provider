{
  "name": "ergebnis/data-provider",
  "description": "Provides generic data providers for use with phpunit/phpunit.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "phpunit",
    "data-provider"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/data-provider",
  "support": {
    "issues": "https://github.com/ergebnis/data-provider/issues",
    "source": "https://github.com/ergebnis/data-provider"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "fakerphp/faker": "^1.19.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.23.1",
    "ergebnis/license": "^1.2.0",
    "ergebnis/php-cs-fixer-config": "^4.1.0",
    "phpunit/phpunit": "^9.5.16",
    "psalm/plugin-phpunit": "~0.16.1",
    "vimeo/psalm": "^4.21"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\DataProvider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\DataProvider\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "platform": {
      "php": "7.4.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
