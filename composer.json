{
  "name": "ergebnis/data-provider",
  "description": "Provides generic data providers for use with phpunit/phpunit.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "phpunit",
    "data-provider"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/data-provider",
  "support": {
    "issues": "https://github.com/ergebnis/data-provider/issues",
    "source": "https://github.com/ergebnis/data-provider"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0",
    "fakerphp/faker": "^1.21.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.37.0",
    "ergebnis/license": "^2.2.0",
    "ergebnis/php-cs-fixer-config": "~6.6.0",
    "ergebnis/phpunit-slow-test-detector": "^2.3.0",
    "infection/infection": "~0.27.3",
    "phpunit/phpunit": "^10.4.0",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.18.5",
    "vimeo/psalm": "^5.15.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\DataProvider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\DataProvider\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.20"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
