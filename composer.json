{
  "name": "ergebnis/data-provider",
  "type": "library",
  "description": "Provides generic data providers for use with phpunit/phpunit.",
  "keywords": [
    "phpunit",
    "data-provider"
  ],
  "homepage": "https://github.com/ergebnis/data-provider",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.16.0",
    "ergebnis/license": "^1.1.0",
    "ergebnis/php-cs-fixer-config": "^3.2.1",
    "ergebnis/test-util": "^1.5.0",
    "phpunit/phpunit": "~9.5.10",
    "psalm/plugin-phpunit": "~0.16.1",
    "vimeo/psalm": "^4.13.1"
  },
  "config": {
    "platform": {
      "php": "7.3.32"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\DataProvider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\DataProvider\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/data-provider/issues",
    "source": "https://github.com/ergebnis/data-provider"
  }
}
